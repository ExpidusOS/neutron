name: FlutterEngine
description: |
  Bindings for the Flutter Engine
output:
  bindings: 'lib/bindings.dart'
  symbol-file:
    output: 'package:flutter_engine/bindings.yaml'
    import-path: 'package:flutter_engine/bindings.dart'
headers:
  entry-points:
    - '@FLUTTER_ENGINE_DIR@/flutter_embedder.h'
  include-directives:
    - '@FLUTTER_ENGINE_DIR@/flutter_embedder.h'
preamble: |
  // ingore_for_file: always_specify_types
  // ignore_for_file: camel_case_types
  // ignore_for_file: non_constant_identifier_name
comments:
  style: any
  length: full
